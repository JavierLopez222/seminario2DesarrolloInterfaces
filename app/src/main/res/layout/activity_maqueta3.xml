<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Maqueta3">
    <LinearLayout
        android:id="@+id/ll1"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/ll1.1"
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/ll1.2"
            android:background="#3FFF82"
            android:gravity="center">
            <androidx.cardview.widget.CardView
                android:id="@+id/cardview1"
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/logocasa"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll1.2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintStart_toEndOf="@+id/ll1.1"
            app:layout_constraintTop_toTopOf="parent"
            android:background="#3FFF82"
            android:gravity="center">
            <TextView
                android:id="@+id/textviewTitulo"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="CASANUOVO --    CHAT   --"
                android:textColor="@color/white"
                android:textSize="25sp"
                android:textStyle="bold"/>
            <Space
                android:layout_width="80dp"
                android:layout_height="0dp"/>
            <androidx.cardview.widget.CardView
                android:id="@+id/cardview2"
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:cardCornerRadius="50dp">
                <ImageView
                    android:id="@+id/imageViewlupa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/logolupa"/>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll2"
        android:layout_width="match_parent"
        android:layout_height="650dp"
        app:layout_constraintTop_toBottomOf="@+id/ll1"
        app:layout_constraintStart_toStartOf="parent"
        android:orientation="vertical">
        <ScrollView
            android:id="@+id/scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_marginEnd="5dp"
            android:layout_marginStart="5dp"
            android:paddingHorizontal="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll3"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll1"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll3.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll3.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll3.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Hola vecino, ¿Cómo estás hoy? ¿Has encontrado tu piso ideal?"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll4"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll3"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll4.1"
                        android:layout_width="295dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll3.1"
                        android:gravity="end">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Buenas tardes, la respuesta es que no!!!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll4.2"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miidos"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll5"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll4"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll5.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll5.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll5.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Gracias a esta aplicación, lo conseguirás!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll6"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll5"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll6.1"
                        android:layout_width="295dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll6.1"
                        android:gravity="end">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Eso espero, por que no me queda mucho más tiempo de alquiler y necesito algo nuevo ya!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll6.2"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miidos"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll7"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll6"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll7.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll7.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll7.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="¿Qué te parece si comenzamos a buscar?"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll8"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll7"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll8.1"
                        android:layout_width="295dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll8.1"
                        android:gravity="end">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Me parece muy bien!!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll8.2"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miidos"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll9"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll8"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll9.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll9.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll9.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="¿Cuál es la zona en donde te ves viviendo?"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll10"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll9"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll10.1"
                        android:layout_width="295dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll10.1"
                        android:gravity="end">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="En la zona del centro, para mí es la mejor sin duda!!!!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll10.2"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miidos"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll11"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll10"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll11.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll11.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll11.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Pues comencemos a buscar por ahí!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll12"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll11"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll12.1"
                        android:layout_width="295dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll12.1"
                        android:gravity="end">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="Genial!!!!"
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll12.2"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miidos"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll13"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/ll12"
                    app:layout_constraintStart_toStartOf="parent"
                    android:orientation="horizontal"
                    android:background="#6FCABF">
                    <LinearLayout
                        android:id="@+id/ll13.1"
                        android:layout_width="60dp"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <androidx.cardview.widget.CardView
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            app:cardCornerRadius="50dp"
                            android:background="#59FFEB">
                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/miiuno"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll13.2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        app:layout_constraintStart_toEndOf="@+id/ll13.1"
                        android:gravity="">
                        <androidx.cardview.widget.CardView
                            android:layout_width="250dp"
                            android:layout_height="70dp"
                            android:layout_gravity="center"
                            app:cardCornerRadius="20dp">
                            <TextView
                                android:layout_width="250dp"
                                android:layout_height="70dp"
                                android:background="#FFFFFF"
                                android:text="A continuación te voy a pasar una serie de viviendas perfectas para tus condiciones."
                                android:textStyle="bold"
                                android:gravity="center"/>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </ScrollView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>